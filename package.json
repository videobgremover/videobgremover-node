{
  "name": "videobgremover",
  "version": "0.1.0",
  "description": "Official Node.js SDK for VideoBGRemover API - Remove video backgrounds with AI and compose videos with FFmpeg",
  "keywords": [
    "video",
    "background-removal",
    "ai",
    "ffmpeg",
    "video-editing",
    "composition",
    "transparent-video"
  ],
  "author": "Paul <paul@videobgremover.com>",
  "license": "MIT",
  "homepage": "https://videobgremover.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/videobgremover/videobgremover-node.git"
  },
  "bugs": {
    "url": "https://github.com/videobgremover/videobgremover-node/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:functional": "jest --testPathPattern=functional",
    "test:integration": "jest --testPathPattern=integration",
    "test:watch": "jest --watch",
    "lint": "eslint src/ tests/ --ext .ts,.js",
    "lint:fix": "eslint src/ tests/ --ext .ts,.js --fix",
    "format": "prettier --write src/ tests/",
    "format:check": "prettier --check src/ tests/",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run clean && npm run build && npm run test:unit",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@types/adm-zip": "^0.5.7",
    "adm-zip": "^0.5.16",
    "axios": "^1.6.0",
    "mime-types": "^2.1.35",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios-mock-adapter": "^1.22.0",
    "dotenv": "^16.3.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  }
}
